
{
		// Use IntelliSense to learn about possible attributes.
		// Hover to view descriptions of existing attributes.
		// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
		"version": "0.2.0",
		"configurations": [
			{
				"name": "Launch Package",
				"type": "go",
				"request": "launch",
				"mode": "debug",
				"program": "${workspaceFolder}/cmd/main.go",
				"cwd": "${workspaceFolder}",
				"args": [
					"--allow-privileged=true",
					"--apiserver-count=1",
					"--cors-allowed-origins=.*",
					"--delete-collection-workers=1",
					"--etcd-prefix=/kubezoo",
					"--etcd-servers=http://localhost:2379",
					"--event-ttl=1h0m0s",
					"--logtostderr=true",
					"--max-requests-inflight=1002",
					"--service-cluster-ip-range=192.168.0.1/16",
					"--service-node-port-range=20000-32767",
					"--storage-backend=etcd3",
					"--authorization-mode=AlwaysAllow",
					"--client-ca-file=${workspaceFolder}/_output/pki/kubezoo/ca.pem",
					"--client-ca-key-file=${workspaceFolder}/_output/pki/kubezoo/ca-key.pem",
					"--tls-cert-file=${workspaceFolder}/_output/pki/kubezoo/kubernetes.pem",
					"--tls-private-key-file=${workspaceFolder}/_output/pki/kubezoo/kubernetes-key.pem",
					"--service-account-key-file=${workspaceFolder}/_output/pki/upstream/sa.pub",
					"--service-account-issuer=kubezoo",
					"--service-account-signing-key-file=${workspaceFolder}/_output/pki/upstream//sa.key",
					"--proxy-client-cert-file=${workspaceFolder}/_output/pki/upstream/client.crt",
					"--proxy-client-key-file=${workspaceFolder}/_output/pki/upstream/client-key.crt",
					"--proxy-client-ca-file=${workspaceFolder}/_output/pki/upstream/ca.crt",
					"--request-timeout=10m",
					"--watch-cache=true",
					"--proxy-upstream-master=https://127.0.0.1:64727",
					"--service-account-lookup=false",
					"--proxy-bind-address=127.0.0.1",
					"--proxy-secure-port=6443",
					"--api-audiences=kubezoo",
					"--v=4",
				],
			},
		]
	}
	